<?xml version="1.0" encoding="ISO-8859-1"?>
<resource>
  <object class="wxDialog" name="dlgRule">
    <title></title>
    <size>218,240d</size>
    <style>wxDEFAULT_DIALOG_STYLE|wxCAPTION|wxSYSTEM_MENU|wxRESIZE_BORDER|wxRESIZE_BOX|wxTHICK_FRAME</style>
    <object class="wxFlexGridSizer">
      <cols>1</cols>
      <rows>3</rows>
      <object class="sizeritem">
        <object class="wxNotebook" name="nbNotebook">
          <object class="notebookpage">
            <label>Properties</label>
            <object class="wxPanel" name="pnlProperties">
              <object class="wxStaticText" name="stName">
                
                <label>Name</label>
                
                <pos>5,7d</pos>
              </object>
              <object class="wxTextCtrl" name="txtName">
                
                <pos>70,5d</pos>
                
                <size>135,-1d</size>
              </object>
              <object class="wxStaticText" name="stOID">
                
                <label>OID</label>
                
                <pos>5,22d</pos>
              </object>
              <object class="wxTextCtrl" name="txtOID">
                
                <pos>70,20d</pos>
                
                <size>135,-1d</size>
              </object>
              <object class="wxStaticText" name="stEvent">
                
                <label>Event</label>
                
                <pos>5,37d</pos>
              </object>
              <object class="wxRadioBox" name="rbxEvent">
                
                <label></label>
                
                <content>
                  <item>SELECT</item>
                  <item>INSERT</item>
                  <item>UPDATE</item>
                  <item>DELETE</item>
                </content>
                
                <pos>70,33d</pos>
                
                <dimension>1</dimension>
                
                <size>135,-1d</size>
                
                <style>wxRA_SPECIFY_COLS</style>
              </object>
              <object class="wxStaticText" name="stDoInstead">
                
                <label>Do instead</label>
                
                <pos>5,97d</pos>
              </object>
              <object class="wxCheckBox" name="chkDoInstead">
                
                <label></label>
                
                <pos>70,95d</pos>
                
                <size>13,12d</size>
              </object>
              <object class="wxStaticText" name="stCondition">
                
                <label>Condition</label>
                
                <pos>5,112d</pos>
              </object>
              <object class="wxTextCtrl" name="txtCondition">
                
                <pos>70,110d</pos>
                
                <size>135,36d</size>
                
                <style>wxTE_MULTILINE</style>
              </object>
              <object class="wxStaticText" name="stComment">
                
                <label>Comment</label>
                
                <pos>5,152d</pos>
              </object>
              <object class="wxTextCtrl" name="txtComment">
                
                <pos>70,150d</pos>
                
                <size>135,37d</size>
                
                <style>wxTE_MULTILINE</style>
              </object>
              <object class="wxStaticText" name="stClusterSet">
                <label>Use replication</label>
                <pos>5,192d</pos>
              </object>
              <object class="wxComboBox" name="cbClusterSet">
                <content/>
                <pos>70,190d</pos>
                <size>135,12d</size>
                <style>wxCB_READONLY|wxCB_DROPDOWN</style>
              </object>
            </object>
            
            <selected>1</selected>
          </object>
          <pos>2,2d</pos>
          <size>214,215d</size>
          <object class="notebookpage">
            <label>Definition</label>
            <object class="wxPanel" name="pnlDefinition">
              <object class="wxBoxSizer">
                <orient>wxVERTICAL</orient>
                <object class="sizeritem">
                  <object class="ctlSQLBox" name="txtSqlBox">
                    
                    <style>wxTE_MULTILINE</style>
                  </object>
                  
                  <option>1</option>
                  
                  <flag>wxGROW</flag>
                </object>
              </object>
            </object>
          </object>
        </object>
        
        <flag>wxALL|wxGROW|wxALIGN_CENTRE</flag>
        
        <border>3</border>
      </object>
      <object class="sizeritem">
        <object class="wxFlexGridSizer">
          <cols>5</cols>
          <rows>1</rows>
          <object class="sizeritem">
            <object class="wxButton" name="wxID_HELP">
              
              <label>&amp;Help</label>
              
              <pos>135,220d</pos>
            </object>
          </object>
          <object class="spacer">
            
            <size>0,0d</size>
          </object>
          <object class="sizeritem">
            <object class="wxButton" name="wxID_OK">
              
              <label>&amp;OK</label>
              
              <default>1</default>
              
              <pos>135,220d</pos>
            </object>
          </object>
          <object class="spacer">
            
            <size>2,17d</size>
          </object>
          <object class="sizeritem">
            <object class="wxButton" name="wxID_CANCEL">
              
              <label>&amp;Cancel</label>
              
              <pos>176,220d</pos>
            </object>
          </object>
          <growablecols>1</growablecols>
        </object>
        
        <flag>wxTOP|wxLEFT|wxRIGHT|wxGROW</flag>
        
        <border>3</border>
      </object>
      <growablecols>0</growablecols>
      <growablerows>0</growablerows>
      <object class="sizeritem">
        <object class="unknown" name="unkStatusBar">
          
          <size>-1,15d</size>
        </object>
        
        <flag>wxGROW|wxALIGN_CENTRE</flag>
        
        <border>3</border>
      </object>
    </object>
  </object>
</resource>
